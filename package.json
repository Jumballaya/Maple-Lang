{
  "name": "maple-lang",
  "version": "0.0.1",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "tsc",
    "test": "tsx --test \"test/**/*.test.ts\"",
    "test:watch": "tsx --test --watch \"test/**/*.test.ts\"",
    "test:spec": "NODE_OPTIONS=--test-reporter=spec tsx --test \"test/**/*.test.ts\"",
    "test:ci": "tsx --test \"test/**/*.test.ts\"",
    "coverage": "c8 -r text -r html node --test \"test/**/*.test.ts\" --loader tsx",
    "clean": "rm -rf dist && rm -rf coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.7.2",
    "c8": "^10.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
